@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix mc: <http://www.inria.org/entity/> .
@prefix mp: <http://www.inria.org/property/> .
@prefix ms: <http://www.inria.org/shape/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ms:DiseaseShape
    a sh:Shape ;
    sh:targetClass mc:Disease
    sh:not [
        sh:class
            mc:Patient ,
            mc:Consultation ,
            mc:Symptom , # peut être à enlever (hypocalcemia = "crise de tétatie")
            mc:Doctor ,
            mc:Medicine
    ] ;
    sh:property [
        sh:path mp:hasSymptom ;
        sh:datatype mc:Symptom ;
        sh:minCount 1
    ] .

ms:PatientShape
    a sh:Shape ;
    sh:targetClass mc:Patient ;
    sh:not [
        mc:Disease ,
        mc:Consultation ,
        mc:Symptom ,
        mc:Doctor ,
        mc:Medicine
    ] ;
    sh:property [
        sh:path mp:consulted ;
        sh:datatype mc:Consultation ;
        sh:minCount 1
    ] .

ms:ConsultationShape
    a sh:Shape ;
    sh:targetClass mc:Consultation ;
    sh:not [
        mc:Disease ,
        mc:Patient ,
        mc:Symptom ,
        mc:Doctor ,
        mc:Medicine
    ] ;
    sh:property
        [
            sh:path mp:declaredSymptom ;
            sh:datatype mc:Symptom ;
            sh:minCount 1
        ] ,
        [
            sh:path mp:docInCharge ;
            sh:datatype mc:Doctor ;
            sh:minCount 1
        ] .

ms:SymptomShape
    a sh:Shape ;
    sh:targetClass mc:Symptom ;
    sh:not [
        mc:Disease , # peut être  enlevé (voir crise de tétanie)
        mc:Patient ,
        mc:Consltation ,
        mc:Doctor ,
        mc:Medicine
    ] .


ms:MedicineShape
    a sh:Shape ;
    sh:targetClass mc:Medicine ;
    sh:not [
        mc:Disease ,
        mc:Patient ,
        mc:Consltation ,
        mc:Doctor ,
        mc:Symptom
    ] ;
    sh:property [
        sh:path mc:cures ;
        sh:datatype Disease ;
        sh:minCount 1
    ] .