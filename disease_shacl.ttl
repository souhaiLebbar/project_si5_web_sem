@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix mc: <http://www.inria.org/entity/> .
@prefix mp: <http://www.inria.org/property/> .
@prefix ms: <http://www.inria.org/shape/> .
@prefix sh: <http://www.w3.org/ns/shacl#> .

ms:DiseaseShape
    a sh:Shape ;
    sh:targetClass mc:Disease ;
    sh:not [
        sh:class
            mc:Patient ,
            mc:Consultation ,
            mc:Symptom , # peut être à enlever (hypocalcemia = "crise de tétatie")
            mc:Doctor ,
            mc:Medicine
    ] ;
    sh:property [
        sh:path mp:hasSymptom ;
        sh:datatype mc:Symptom ;
        sh:minCount 1
    ] .

ms:PatientShape
    a sh:Shape ;
    sh:targetClass mc:Patient ;
    sh:not [
        sh:class
            mc:Disease ,
            mc:Consultation ,
            mc:Symptom ,
            mc:Doctor ,
            mc:Medicine
    ] ;
    sh:property [
        sh:path mp:consulted ;
        sh:datatype mc:Consultation ;
        sh:minCount 1
    ] , [
        sh:path mp:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1
    ].

ms:ConsultationShape
    a sh:Shape ;
    sh:targetClass mc:Consultation ;
    sh:not [
        sh:class
            mc:Disease ,
            mc:Patient ,
            mc:Symptom ,
            mc:Doctor ,
            mc:Medicine
    ] ;
    sh:property
        [
            sh:path mp:declaredSymptom ;
            sh:datatype mc:Symptom ;
            sh:minCount 1
        ] ,
        [
            sh:path mp:docInCharge ;
            sh:datatype mc:Doctor ;
            sh:minCount 1 ;
            sh:maxCount 1
        ] ,
        [
            sh:path mp:tookPlace ;
            sh:datatype xsd:dateTime ;
            sh:minCount 1 ;
            sh:maxCount 1
        ] ,
        [
            sh:path mp:triedMed ;
            sh:datatype mc:Medicine
        ] .

ms:SymptomShape
    a sh:Shape ;
    sh:targetClass mc:Symptom ;
    sh:not [
        sh:class
            mc:Disease , # peut être  enlevé (voir crise de tétanie)
            mc:Patient ,
            mc:Consltation ,
            mc:Doctor ,
            mc:Medicine
    ] ;
    sh:property [
        sh:path mp:symptomOf ;
        sh:datatype mc:Disease ;
        sh:minCount 1
    ] .

ms:MedicineShape
    a sh:Shape ;
    sh:targetClass mc:Medicine ;
    sh:not [
        sh:class
            mc:Disease ,
            mc:Patient ,
            mc:Consltation ,
            mc:Doctor ,
            mc:Symptom
    ] ;
    sh:property [
        sh:path mc:cures ;
        sh:datatype mc:Disease ;
        sh:minCount 1
    ] .

ms:DoctorShape
    a sh:Shape ;
    sh:targetClass mc:Doctor ;
    sh:not [
        sh:class
            mc:Disease ,
            mc:Patient ,
            mc:Consltation ,
            mc:Doctor ,
            mc:Symptom
    ] ;
    sh:property [
        sh:path mp:name ;
        sh:datatype xsd:string ;
        sh:minCount 1 ;
        sh:maxCount 1
    ] .